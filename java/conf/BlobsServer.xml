<quickserver>
	<name>BlobsServer v 0.1</name>
	<port>3444</port>
	<bind-address>0.0.0.0</bind-address>

	<client-command-handler>
		edu.columbia.threescompany.server.BlobsCommandHandler
	</client-command-handler>
	
	<client-data>
		edu.columbia.threescompany.server.PlayerServerData
	</client-data>
	
	<client-object-handler>
		edu.columbia.threescompany.server.BlobsClientObjectHandler
	</client-object-handler>
	
	<client-authentication-handler>
		edu.columbia.threescompany.server.BlobsServerQuickAuthenticator
	</client-authentication-handler>
	
	<timeout>-1</timeout>
	
	<!-- Config QSAdminServer -->
	<qsadmin-server>
		<port>4445</port>
		<server-banner>
			QSAdminServer Started on port : 4445
		</server-banner>
		<command-shell>
			<enable>true</enable>
		</command-shell>
		<communication-logging>
			<enable>false</enable>
		</communication-logging>
	</qsadmin-server>
	<!-- Config QSAdminServer -->

</quickserver>